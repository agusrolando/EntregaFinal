<table >
  <thead>
    <tr>
      <th scope="col"></th>
      <th scope="col">Title</th>
      <th scope="col">Categoria</th>
      <th scope="col">Price</th>
      <th scope="col">Stock</th>
      <th scope="col">Owner</th>
      <th scope="col"></th>
    </tr>
  </thead>
  {{#each data.docs}}
  <tbody>
    <tr>
      <th scope="row"></th>
      <td><a href="/api/products/{{_id}}">{{title}}</a></td>
      <td>{{category}}</td>
      <td>{{price}}</td>
      <td>{{stock}}</td>
      <td>{{owner.email}}</td>
      <td>
      </td>
    </tr>
  </tbody>
  {{/each}}
</table>

<nav aria-label="Page navigation example">
    <ul class="pagination">
        <li class="page-item {{#if data.hasPrevPage}}{{else}}disabled{{/if}}">
            <a class="page-link" href="/products?page={{data.prevPage}}">Ant.</a>
        </li>

        {{#each front_pagination}}
            <li class="page-item {{#if this.active}}active{{/if}}">
                <a class="page-link" href="/products?page={{this.page}}">{{this.page}}</a>
            </li>
        {{/each}}

        <li class="page-item {{#if data.hasNextPage}}{{else}}disabled{{/if}}">
            <a class="page-link" href="/products?page={{data.nextPage}}">Sig.</a>
        </li>
    </ul>
</nav>
<a href="/api/products"><button>Crear Producto</button></a>
<a href="/session/current"><button>Perfil</button></a>
<a href="/api/carts/{{user.cart._id}}"><button>Carrito</button></a>
<a href="/api/carts/purchase"><button>Tickets</button></a>

<script src="/js/product.js"></script>